@{
  Layout = "_Layout";
}
<div class="container">
<h1>Stylists</h1>
@if (@Model.Count == 0)
{
<h4>No stylists has been added yet</h4>
}

@foreach (var stylist in Model)
{
<h4><a href='/stylists/@stylist.GetId()'>@stylist.GetName()</a></h4>
}

<p><a href="/stylists/new" class="btn btn-info">Add new Stylist</a></p>
<p><a href="/"class="btn btn-danger" >back Home</a></p>
</div>
