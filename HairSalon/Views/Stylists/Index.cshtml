@{
  Layout = "_Layout";
}

<h1>Stylists</h1>
@if (@Model.Count == 0)
{
<h4>No stylists has been added yet</h4>
}

@foreach (var stylist in Model)
{
<h4><a href='/stylists/@stylist.GetId()'>@stylist.GetName()</a></h4>
}

<p><a href="/stylists/new">Add new Stylist</a></p>
<p><a href="/">back Home</a></p>
